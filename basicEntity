  <script>
    // Title strobe + status bar spam (authentically obnoxious)
    (function(){
      var x = 0; var msg = [
        'WELCOME TO THE FUTURE (1997)',
        'BEST VIEWED IN NETSCAPE 3.0',
        'UNDER CONSTRUCTION!!!',
        'TURN ON YOUR SPEAKERS!!!',
        'SIGN MY GUESTBOOK!!!'
      ];
      setInterval(function(){ document.title = msg[x = (x+1)%msg.length]; }, 500);
      setInterval(function(){ window.status = msg[Math.floor(Math.random()*msg.length)]; }, 400);
    })();

    // Cursor trail stars (because why not)
    document.addEventListener('mousemove', function(e){
      var s = document.createElement('span');
      s.innerHTML = '‚ú¶';
      s.style.position = 'fixed';
      s.style.left = e.clientX + 'px';
      s.style.top = e.clientY + 'px';
      s.style.pointerEvents = 'none';
      s.style.fontSize = (8 + Math.random()*24) + 'px';
      s.style.color = 'hsl(' + (Math.random()*360) + ',100%,60%)';
      s.style.transition = 'all 1s ease-out';
      document.body.appendChild(s);
      setTimeout(function(){ s.style.transform = 'translateY(-60px)'; s.style.opacity = 0; }, 10);
      setTimeout(function(){ s.remove(); }, 1000);
    });

    function addCounter(){
      var n = parseInt(localStorage.getItem('hits')||'0',10)+1;
      localStorage.setItem('hits', n);
      document.getElementById('hits').innerText = ('0000000' + n).slice(-7);
    }
    window.onload = addCounter;
  </script>
